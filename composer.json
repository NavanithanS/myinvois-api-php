{
    "name": "nava/myinvois",
    "description": "PHP client library for Malaysia's MyInvois API for tax document submissions",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "myinvois",
        "malaysia",
        "e-invoice",
        "laravel",
        "tax"
    ],
    "autoload": {
        "psr-4": {
            "Nava\\MyInvois\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nava\\MyInvois\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "endroid/qr-code": "3.5.9",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "psr/simple-cache": "^1.0|^2.0|^3.0",
        "spatie/data-transfer-object": "^1.0",
        "webmozart/assert": "^1.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^2.0|^3.0|^5.0",
        "orchestra/testbench": "^6.28",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.6",
        "nesbot/carbon": "^1.39.1|^2.72.6",
        "laravel/framework": "^8.83.27",
        "league/flysystem": "^1.1.4",
        "symfony/http-foundation": "^3.4.47|^4.4.50|^5.4.46",
        "symfony/process": "^3.4.47|^4.4.50|^5.4.46",
        "symfony/http-kernel": "^3.4.50|^4.4.50|^5.4.46"
    },
    "suggest": {
        "laravel/framework": "Required to use the Laravel service provider and facades"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nava\\MyInvois\\Laravel\\MyInvoisServiceProvider"
            ],
            "aliases": {
                "MyInvois": "Nava\\MyInvois\\Laravel\\Facades\\MyInvois"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
